   "source": [
    "def f(indent, n):\n",
    "    if n == 0:\n",
    "        return\n",
    "    print(\" \" * indent + \"*\" * n)\n",
    "    f(indent + 1, n - 1)\n",
    "    print(\" \" * indent + \"*\" * n)\n",
    "\n",
    "\n",
    "print(\">>> f(0, 0)\")\n",
    "f(0, 0)\n",
    "\n",
    "print(\"\\n>>> f(0, 1)\")\n",
    "f(0, 1)\n",
    "\n",
    "print(\"\\n>>> f(0, 2)\")\n",
    "f(0, 2)\n",
    "\n",
    "print(\"\\n>>> f(0, 4)\")\n",
    "f(0, 4)\n"
   ]
